#include <stdio.h>

int main(void)
{
	float tal,x;

	printf("Enter a number:");
	scanf("%f",&tal);

	x=tal;

	while(x<tal+10)
	{

		printf("%.0f\n",x);

		x=x+1;
	}

	return 0;
}
