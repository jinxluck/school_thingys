#include<iostream>
#include<unistd.h> //for usleep
#include"GPIO_controle.h"

using namespace std;

int GPIO_IN = 57; //P2.06
int GPIO_OUT = 60; //P2.08

int main(void)
{
	printf("V1.6\n\n");
	int val= 0;

	printf(" *setting direction\n");
	set_direction(GPIO_IN, 0);

	printf(" *setting new value\n");
	set_value(GPIO_IN, 1);
	val = read_value(GPIO_IN);
	printf("%d\n",val);

	printf(" *setting new value\n");
	set_value(GPIO_IN, 0);
	val = read_value(GPIO_IN);
	printf("%d\n",val);

	return 0;
}
